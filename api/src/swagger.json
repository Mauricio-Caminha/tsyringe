                },
                "email": {
                    "type": "string"
                },
                "driver_license": {
                    "type": "string"
                },
                "admin": {
                    "type": "boolean"
                },
                "avatar": {
                    "type": "string"
                },
                "created_at": {
                    "type": "string"
                }
            },
            "UserCreation": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "password": {
                        "type": "string"
                    },
                    "email": {
                        "type": "string"
